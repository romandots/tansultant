{
  "name": "tansultant/api",
  "version": "0.1",
  "type": "project",
  "description": "Tansultant CRM",
  "keywords": [
    "crm",
    "tansultant"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "barryvdh/laravel-dompdf": "^3.1",
    "clue/socket-raw": "^1.6",
    "doctrine/annotations": "^2.0",
    "hedii/laravel-gelf-logger": "^10.1",
    "itsgoingd/clockwork": "^5.2",
    "laravel/horizon": "^5.9",
    "laravel/laravel": "^12.0",
    "laravel/sanctum": "^4.1",
    "laravel/reverb": "^1.5",
    "lee-to/php-tochka-api-v2-sdk": "^1.0",
    "predis/predis": "^3.0",
    "pusher/pusher-php-server": "^7.2",
    "spatie/laravel-health": "^1.34",
    "spatie/laravel-medialibrary": "^11.13",
    "spatie/laravel-permission": "^6.19",
    "spatie/laravel-tags": "^4.10",
    "srwiez/queue-size-health-check": "^1.2",
    "symfony/expression-language": "^7.3.0",
    "vladimir-yuldashev/laravel-queue-rabbitmq": "^14.2",
    "wapmorgan/morphos": "^3.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "require-dev":
  {
    "fakerphp/faker": "^1.21",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^8.8",
    "phpunit/phpunit": "^12.2"
  }
}
